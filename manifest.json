{
    "name": "Google Search Console Enhanced Analytics",
    "version": "2.0.1",
    "description": "Google Search Console Enhanced Analytics is a simple, time-saving extension that enhances your analysis.",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "https://search.google.com/*"
    ],
    "background": {
        "service_worker": "/files/js/background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/files/favicon/favicon16.png",
            "32": "/files/favicon/favicon32.png"
        }
    },
    "icons": {
        "16": "/files/favicon/favicon16.png",
        "32": "/files/favicon/favicon32.png",
        "48": "/files/favicon/icon48.png",
        "128": "/files/favicon/icon128.png"
    }
}
